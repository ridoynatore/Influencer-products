function sendRelatedPostsGAEvent(e) {
  if (window.ga) {
    tracker = window.ga.getAll()[0];
    if (tracker) {
      tracker.send({
        hitType: 'event',
        eventCategory: 'related blog post',
        eventAction: 'click',
        eventLabel: e.pathname.split("/")[2]
      });
    }
  }
}
</script>

